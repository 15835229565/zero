{
  "fireasy": {
    "dataGlobal": {
      "options": {
        "attachQuote": true
      }
    },
    "dataInstances": {
      "settings": {
        "sqlite": {
          "providerType": "SQLite",
          "connectionString": "Data source=|datadirectory|../../../../database/zero.db3;tracking=true"
        }
      }
    },
    "containers": {
      "settings": {
        "default": [
          {
            "assembly": "Fireasy.Zero.Services"
          },
          {
            "assembly": "Fireasy.Zero.Infrastructure"
          }
        ]
      }
    }
  }
}
